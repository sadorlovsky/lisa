{
  "name": "lissie",
  "version": "0.1.0",
  "description": "tool for generating LICENSE",
  "main": "dist/lissie.js",
  "bin": {
    "lissie": "bin/lissie.js"
  },
  "preferGlobal": true,
  "scripts": {
    "pretest": "npm run build",
    "test": "nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "babel src -d dist",
    "prepublish": "npm run test && npm run build"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "src",
      "bin"
    ]
  },
  "files": [
    "bin",
    "dist",
    "licenses"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sadorlovsky/lissie.git"
  },
  "keywords": [
    "license",
    "generator",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/sadorlovsky/lissie/issues"
  },
  "homepage": "https://github.com/sadorlovsky/lissie",
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "coveralls": "^2.11.9",
    "execa": "^0.2.2",
    "nyc": "^6.2.1",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "meow": "^3.7.0"
  }
}
