{
  "name": "lissie",
  "version": "0.1.0",
  "description": "tool for generating LICENSE",
  "main": "dist/lissie.js",
  "scripts": {
    "test": "nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src -d dist",
    "prepublish": "npm run test && npm run build"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "files": [
    "dist",
    "licenses"
  ],
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "license": "MIT",
  "repository": "sadorlovsky/lissie",
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "babel-register": "^6.7.2",
    "coveralls": "^2.11.9",
    "nyc": "^6.2.1",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "meow": "^3.7.0"
  }
}
