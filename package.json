{
  "name": "lissie",
  "version": "0.2.0",
  "description": "tool for generate LICENSE",
  "main": "dist/lissie.js",
  "bin": {
    "lissie": "bin/lissie.js"
  },
  "preferGlobal": true,
  "scripts": {
    "lint": "eslint src test",
    "pretest": "npm run build && rimraf coverage",
    "test": "NODE_ENV=test nyc --check-coverage --lines 100 ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "babel src -d dist"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "files": [
    "bin",
    "dist",
    "licenses"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sadorlovsky/lissie.git"
  },
  "keywords": [
    "license",
    "generator",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/sadorlovsky/lissie/issues"
  },
  "homepage": "https://github.com/sadorlovsky/lissie",
  "devDependencies": {
    "@sadorlovsky/eslint-config": "^0.13.0",
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.0.0",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "common-tags": "^1.3.1",
    "coveralls": "^2.11.12",
    "eslint": "^3.3.0",
    "eslint-plugin-ava": "^3.1.1",
    "eslint-plugin-fp": "^2.2.0",
    "eslint-plugin-import": "^2.0.1",
    "execa": "^0.5.0",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "assign-defined": "^1.0.5",
    "bluebird": "^3.4.1",
    "fullname": "^3.1.0",
    "meow": "^3.7.0",
    "pify": "^2.3.0",
    "project-name": "^0.2.6",
    "promise.any": "^0.1.0",
    "read-pkg": "^2.0.0",
    "read-pkg-up": "^2.0.0",
    "user-email": "^1.0.1",
    "user-fullname": "^0.3.0"
  }
}
